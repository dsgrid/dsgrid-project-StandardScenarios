{
    project_id: "dsgrid_conus_2022_dpv_calcs",
    description: "Side project to calculate 2010-2050 DPV generation profiles. Temporary workaround ahead of implementing https://github.com/dsgrid/dsgrid/issues/229.",
    name: "dsgrid CONUS 2022 DPV Calculations",
    datasets: [
        {
            dataset_id: "dsgrid_efs_2012_dpv",
            dataset_type: "historical",
            version: "0.2.0",
            required_dimensions: {
                single_dimensional: {
                    model_year: {
                        base: ["2012"],
                    },
                    sector: {
                        base: ["res", "com", "ind"]
                    }
                }
            }
        },
        {
            dataset_id: "dsgrid_efs_2012_dpv_capacity",
            dataset_type: "historical",
            version: "0.1.0",
            required_dimensions: {
                single_dimensional: {
                    model_year: {
                        base: ["2012"],
                    },
                    sector: {
                        base: ["res", "com", "ind"]
                    }
                }
            }
        },
        {
            dataset_id: "dgen_stdscen_2022_dpv_capacity",
            dataset_type: "modeled",
            version: "0.1.0",
            required_dimensions: {
                single_dimensional: {
                    model_year: {
                        base: ["2014", "2016", "2018", "2020", "2022", "2024", "2026", "2028", "2030", "2032", "2034", "2036", "2038", "2040", "2042", "2044", "2046", "2048", "2050"],
                    },
                    sector: {
                        base: ["res", "com", "ind"]
                    }
                }
            }
        },
    ],
    dimensions: {
        base_dimensions: [
            {
                class: 'County',
                description: 'US counties for L48, Census 2020 vintage, no territories',
                file: '../../../dimensions/counties.csv',
                module: 'dsgrid.dimension.standard',
                name: 'US Counties 2020 L48',
                display_name: 'County',
                type: 'geography',
            },
            {
                class: 'Sector',
                description: 'dsgrid Standard Scenarios 2021 Sectors; Commercial, Residential, Industrial and Transportation',
                file: '../../../dimensions/sectors.csv',
                module: 'dsgrid.dimension.standard',
                name: 'Sectors - Com-Res-Ind-Trans',
                display_name: 'Sector',
                type: 'sector',
            },
            {
                type: "subsector",
                name: "unspecified-subsectors",
                display_name: "Unspecified Subsectors",
                file: "../../historical/dsgrid_efs_2012_dpv/dimensions/subsectors.csv",
                module: "dsgrid.dimension.standard",
                "class": "Subsector",
                description: "Unspecified Subsectors",
            },
            {
                type: "metric",
                name: "dpv",
                display_name: "Distributed PV",
                file: "../../historical/dsgrid_efs_2012_dpv/dimensions/metric.csv",
                module: "dsgrid.dimension.standard",
                "class": "EnergyEndUse",
                description: "Distributed PV generation",
            },
            {
                type: "model_year",
                name: "Model-Years-Even-2012_to_2050",
                display_name: "None",
                file: "dimensions/model_years_even_2012_to_2050.csv",
                module: "dsgrid.dimension.standard",
                "class": "ModelYear",
                description: "Model Years, even years 2012-2050",
            },
            {
                type: "scenario",
                name: "Conus-2022-reference",
                display_name: "Reference",
                file: "../../historical/dsgrid_efs_2012_dpv/dimensions/scenario.csv",
                module: "dsgrid.dimension.standard",
                "class": "Scenario",
                description: "CONUS (Continental U.S.) 2022 Reference",
            },
            {
                // Note: This matches dsgrid EFS time rather than final project time
                name: "Time-2012-EST-hourly-periodEnding-noDST-noLeapDayAdjustment-total-UTChour1start",
                display_name: "2012 Hourly EST",
                type: "time",
                module: "dsgrid.dimension.standard",
                "class": "Time",
                description: "Time dimension, 2012 hourly EST, period-ending, no DST, no Leap Day Adjustment, total value",
                time_type: "datetime",
                measurement_type: "total",
                str_format: "%Y-%m-%d %H:%M:%S",
                frequency: "P0DT1H0M0.000000S",
                leap_day_adjustment: "none",
                time_interval_type: "period_ending",
                timezone: "EasternStandard",
                ranges: [
                    {
                    start: "2012-01-01 01:00:00",
                    end: "2013-01-01 00:00:00",
                    },
                ],
            },
            {
                class: 'WeatherYear',
                description: 'Weather year, 2012 only',
                file: '../../../dimensions/weather_years.csv',
                module: 'dsgrid.dimension.standard',
                name: '2012 Weather Year',
                display_name: 'Weather 2012',
                type: 'weather_year',
            },
        ]
    }
}