{
  dataset_id: "aeo2021_reference_residential_energy_use_growth_factors",
  dataset_type: "modeled",
  data_source: "NEMS Residential Module",
  description: "AEO 2021 Reference Case Residential Energy End Use Annual Growth Factors. Derived by NREL as simple exponential growth rates from annual end use comsumptions between year 2018 and year 2050. Detailed end use consumption data provided by EIA, not available in API.",
  origin_creator: "Kevin Jarzomski",
  origin_organization: "EIA",
  origin_contributors: [
    "Lixi Liu",
  ],
  origin_project: "dsgrid CONUS2022 Residential Buildings Projection",
  data_source_date: "2021-10-05",
  data_source_version: "0.1.0",
  data_source_doi_url: "Data derived from AEO 2021 detailed residential sector files sent from EIA",
  data_classification: "low",
  tags: [
    "residential",
    "benchmark",
    "projection",
    "aeo2021",
    "dsgrid_v0.2.0",
  ],
  trivial_dimensions: [
    "sector",
    "subsector",
    "scenario",
    "geography",
    "weather_year",
    "model_year",
  ],
  table_schema: {
    data_file: {
      path: "load_data.parquet",
    },
    data_schema: {
      table_format: {
        format_type: "pivoted",
        pivoted_dimension_type: "metric",
      },
      data_schema_type: "one_table",
    },
  },
  dataset_qualifier_metadata: {
    growth_rate_type: "exponential_annual",
    dataset_qualifier_type: "growth_rate",
  },
  dimensions: [
    {
      "class": "Sector",
      description: "Residential sector only",
      file: "../../../resstock/dimensions/sector_residential.csv",
      module: "dsgrid.dimension.standard",
      name: "sector",
      type: "sector",
    },
    {
      "class": "Subsector",
      description: "Residential subsector only",
      file: "dimensions/residential_subsectors.csv",
      module: "dsgrid.dimension.standard",
      name: "subsector",
      type: "subsector",
    },
    {
      "class": "EnergyEndUse",
      description: "AEO 2021 residential energy end uses, condensed to map to ResStock (N=18 from 6 end uses, 5 fuel types)",
      file: "dimensions/aeo2021_residential_condensed_energy_enduses.csv",
      module: "dsgrid.dimension.standard",
      type: "metric",
      name: "aeo_residential_end_use",
    },
    {
      "class": "Scenario",
      description: "EIA AEO Reference Scenario",
      file: "../../commercial/End_Use_Growth_Factors/dimensions/aeo_reference_scenario.csv",
      module: "dsgrid.dimension.standard",
      type: "scenario",
      name: "reference_scenario",
    },
    {
      "class": "WeatherYear",
      description: "Weather year, 2012 only",
      file: "../../../../../dimensions/weather_years.csv",
      module: "dsgrid.dimension.standard",
      name: "weather_2012",
      type: "weather_year",
    },
    {
      "class": "ModelYear",
      description: "Model years, 2013-2050, trivial, single record",
      file: "../../commercial/End_Use_Growth_Factors/dimensions/model_years_trivial_2013_to_2050.csv",
      module: "dsgrid.dimension.standard",
      type: "model_year",
      name: "model_year",
    },
    {
      "class": "NoOpTime",
      description: "No Operation Time",
      time_type: "noop",
      module: "dsgrid.dimension.standard",
      type: "time",
      name: "no_op_time",
    },
  ],
  dimension_references: [
    {
      name: "all_dsgrid_conus_2022_geographies",
      type: "geography",
      version: "1.0.0",
    },
  ],
}
