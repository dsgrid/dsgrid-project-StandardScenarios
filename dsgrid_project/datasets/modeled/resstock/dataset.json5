  data_classification: "low",
  tags: [
    "residential",
    "conus-2022",
    "reference",
    "dsgrid_v0.2.0",
    "standard_scenarios",
  ],
  trivial_dimensions: [
    "sector",
    "weather_year",
    "model_year",
    "scenario",
  ],
  table_schema: {
    data_file: {
      path: "./load_data.parquet",
    },
    lookup_data_file: {
      path: "./load_data_lookup.parquet",
    },
    data_schema: {
      table_format: {
        format_type: "pivoted",
        pivoted_dimension_type: "metric",
      },
      data_schema_type: "standard",
    },
  },
  dimensions: [
    {
      type: "geography",
      name: "county",
      file: "dimensions/conus_2022-resstock_geography_county_fips.csv",
      module: "dsgrid.dimension.standard",
      "class": "County",
      description: "CONUS 2022 - ResStock US county in original FIP code (N=3,108)",
    },
    {
      type: "sector",
      name: "sector",
      file: "dimensions/sector_residential.csv",
      module: "dsgrid.dimension.standard",
      "class": "Sector",
      description: "Residential sector only",
    },
    {
      type: "subsector",
      name: "resstock_building_type",
      file: "dimensions/conus_2022-resstock_subsectors.csv",
      module: "dsgrid.dimension.standard",
      "class": "Subsector",
      description: "CONUS 2022 - ResStock building types (N=5)",
    },
    {
      type: "metric",
      name: "resstock_end_use",
      file: "dimensions/conus_2022-resstock_end_uses.csv",
      module: "dsgrid.dimension.standard",
      "class": "EnergyEndUse",
      description: "CONUS 2022 - ResStock end uses (N=48, 5 fuel types)",
    },
    {
      type: "scenario",
      name: "reference_scenario",
      file: "dimensions/scenario_conus_2022_reference.csv",
      module: "dsgrid.dimension.standard",
      "class": "Scenario",
      description: "Scenario: CONUS (Continental U.S.) 2022 Reference",
    },
    {
      type: "model_year",
      name: "model_year_2018",
      file: "dimensions/model_year_2018.csv",
      module: "dsgrid.dimension.standard",
      "class": "ModelYear",
      description: "Model year, 2018 only",
    },
    {
      "class": "WeatherYear",
      description: "Weather year, 2012 only",
      file: "../../../dimensions/weather_years.csv",
      module: "dsgrid.dimension.standard",
      name: "weather_2012",
      type: "weather_year",
    },
    { // for time wrapped dataset
      "class": "Time",
      frequency: "P0DT1H0M0.000000S",
      name: "time_est",
      ranges: [
        {
          start: "2012-01-01 00:00:00",
          end: "2012-12-31 23:00:00",
        },
      ],
      time_type: "datetime",
      leap_day_adjustment: "none",
      description: "Time dimension, 2012 hourly EST, hour-ending, no Leap Day Adjustment, total value, starts at Hour 0",
      time_interval_type: "period_ending",
      str_format: "%Y-%m-%d %H:%M:%S",
      timezone: "EasternStandard",
      measurement_type: "total",
      type: "time",
    },
